# NOTICE: This file is generated by Puppet and will
# be replaced.
#
# Creates an input to be indexed and switches on
# receiving.

<% target.each do |file| -%>
<%= "[monitor://#{file}]" %>
index = <%= index %>
disabled = <%= if enable == true
  then "0"
  else "1"
end %>

<% end -%>
<% if receiver == true -%>
<%= "[splunktcp://#{port}]" %>
<% end -%>
